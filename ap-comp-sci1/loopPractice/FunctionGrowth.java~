public class FunctionGrowth {

    private static String calcVals(int input) {
	String val = (String) Math.log(input) + "\t";
	val += (String) input + "\t"
	val += (String) input * Math.log(input) + "\t";
	val += (String) input * input + "\t";
	val += (String) input * input * input + "\t";
	val += (String) Math.pow(2,input);
	return val;
    }

    public static void main(String[] args) {
	for(int i = 16; i <= 2048; i**) {
	    System.out.println(calcVals(3));
	}
    }
}
